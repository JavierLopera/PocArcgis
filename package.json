{
	"name": "@correos/ui-kit",
	"version": "0.0.1",
	"description": "Stencil Component Starter",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"es2015": "dist/esm/index.mjs",
	"es2017": "dist/esm/index.mjs",
	"types": "dist/types/index.d.ts",
	"collection": "dist/collection/collection-manifest.json",
	"collection:main": "dist/collection/index.js",
	"unpkg": "dist/correos-ui-kit/correos-ui-kit.js",
	"files": [
		"dist/"
	],
	"scripts": {
		"start": "npm-run-all --parallel stencil storybook",
		"prestart": "npm run build:stencil:dev && npm run wca",
		"stencil": "stencil build --dev --watch --serve",
		"storybook": "start-storybook --quiet -p 4200 -s ./.storybook/stencil-integration/client,./src/assets",
		"build:stencil:dev": "npm run fonts:generator && stencil build",
		"build:stencil": "npm run fonts:generator && npm run clean && stencil build --prod",
		"post:build": "node ./.storybook/stencil-integration/post-build.js",
		"build": "npm-run-all clean build:stencil pre:build:storybook build:storybook post:build post:build:storybook",
		"build:storybook": "build-storybook --loglevel error --quiet",
		"pre:build:storybook": "node ./.storybook/prebuild.js --prod",
		"post:build:storybook": "node ./.storybook/postbuild.js --prod",
		"test": "stencil test --spec --e2e",
		"test.watch": "stencil test --spec --e2e --watchAll",
		"wca": "wca analyze src --format json --outFile .stencil/custom-elements.json",
		"clean": "del-cli dist storybook-static AEM.zip",
		"fonts:generator": "node .storybook/webfonts && webfont ./src/globals/svg/**/*.svg --config ./webfonts.json",
		"generate": "cd .generators && plop",
		"link": "cd dist && npm link"
	},
	"devDependencies": {
		"@alepop/stencil-env": "^0.4.0",
		"@babel/core": "^7.8.4",
		"@stencil/core": "^1.8.8",
		"@stencil/postcss": "^1.0.1",
		"@stencil/react-output-target": "0.0.3",
		"@stencil/sass": "^1.1.1",
		"@storybook/addon-a11y": "^5.3.14",
		"@storybook/addon-actions": "^5.3.14",
		"@storybook/addon-backgrounds": "^5.3.14",
		"@storybook/addon-docs": "^5.3.14",
		"@storybook/addon-events": "^5.3.14",
		"@storybook/addon-jest": "^5.3.14",
		"@storybook/addon-knobs": "^5.3.14",
		"@storybook/addon-links": "^5.3.14",
		"@storybook/addon-notes": "^5.3.14",
		"@storybook/addon-viewport": "^5.3.14",
		"@storybook/addons": "^5.3.14",
		"@storybook/client-api": "^5.3.14",
		"@storybook/core": "^5.3.14",
		"@storybook/core-events": "^5.3.14",
		"@storybook/source-loader": "^5.3.14",
		"@storybook/web-components": "^5.3.14",
		"@types/jest": "^25.1.3",
		"@types/node": "^13.7.7",
		"archiver": "^3.1.1",
		"babel-loader": "^8.0.6",
		"del-cli": "^1.1.0",
		"dotenv": "^8.2.0",
		"fast-glob": "^3.2.2",
		"fs-extra": "^7.0.1",
		"jsonfile": "^6.0.0",
		"lit-html": "^1.1.2",
		"npm-run-all": "^4.1.5",
		"plop": "^2.2.0",
		"postcss-replace": "^1.1.0",
		"replace-in-file": "^5.0.2",
		"rollup-plugin-generate-package-json": "^3.2.0",
		"upath": "^1.2.0",
		"web-component-analyzer": "^1.0.3",
		"webfont": "^9.0.0"
	},
	"license": "MIT",
	"dependencies": {
		"@material/base": "^4.0.0",
		"@material/button": "^4.0.0",
		"@material/card": "^4.0.0",
		"@material/fab": "^4.0.0",
		"@material/menu": "^4.0.0",
		"@material/menu-surface": "^4.0.0",
		"@material/ripple": "^4.0.0",
		"@material/textfield": "^4.0.0",
		"detect-element-resize": "0.0.2",
		"focus-trap": "^5.1.0",
		"lodash": "^4.17.15",
		"resize-observer-polyfill": "^1.5.1",
		"sass-rem": "^2.0.1",
		"tiny-slider": "^2.9.2"
	}
}
